+import { InjectionMode, Stylesheet } from './Stylesheet';
+import { styleToClassName } from './styleToClassName';
+var _stylesheet = Stylesheet.getInstance();
+_stylesheet.setConfig({ injectionMode: InjectionMode.none });
+var shadowConfig;
+describe('styleToClassName', function () {
+    beforeEach(function () {
+        _stylesheet.reset();
+        shadowConfig = { stylesheetKey: '__globalTest__', inShadow: false };
+    });
+    it('can register classes and avoid re-registering', function () {
+        var className = styleToClassName({}, shadowConfig, { background: 'red' });
+        expect(className).toEqual('css-0');
+        expect(_stylesheet.getRules()).toEqual('.css-0{background:red;}');
+        className = styleToClassName({}, shadowConfig, { background: 'red' });
+        expect(className).toEqual('css-0');
+        expect(_stylesheet.getRules()).toEqual('.css-0{background:red;}');
+        className = styleToClassName({}, shadowConfig, { background: 'green' });
+        expect(className).toEqual('css-1');
+        expect(_stylesheet.getRules()).toEqual('.css-0{background:red;}.css-1{background:green;}');
+    });
+    it('can have child selectors', function () {
+        styleToClassName({}, shadowConfig, {
+            selectors: {
+                '.foo': { background: 'red' },
+            },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0 .foo{background:red;}');
+    });
+    it('can have child selectors without the selectors wrapper', function () {
+        styleToClassName({}, shadowConfig, {
+            '.foo': { background: 'red' },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0 .foo{background:red;}');
+    });
+    it('can have child selectors with comma', function () {
+        styleToClassName({}, shadowConfig, {
+            selectors: {
+                '.foo, .bar': { background: 'red' },
+            },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0 .foo{background:red;}.css-0 .bar{background:red;}');
+    });
+    it('can have child selectors with comma without the selectors wrapper', function () {
+        styleToClassName({}, shadowConfig, {
+            '.foo, .bar': { background: 'red' },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0 .foo{background:red;}.css-0 .bar{background:red;}');
+    });
+    it('can have child selectors with comma with pseudo selectors', function () {
+        styleToClassName({}, shadowConfig, {
+            selectors: {
+                ':hover, :active': { background: 'red' },
+            },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0:hover{background:red;}.css-0:active{background:red;}');
+    });
+    it('can have child selectors with comma with pseudo selectors', function () {
+        styleToClassName({}, shadowConfig, {
+            ':hover, :active': { background: 'red' },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0:hover{background:red;}.css-0:active{background:red;}');
+    });
+    it('can have child selectors with comma with @media query', function () {
+        styleToClassName({}, shadowConfig, {
+            selectors: {
+                '@media screen and (-ms-high-contrast: active), (-ms-high-contrast: none)': {
+                    background: 'red',
+                },
+            },
+        });
+        expect(_stylesheet.getRules()).toEqual('@media screen and (-ms-high-contrast: active), (-ms-high-contrast: none){.css-0{background:red;}}');
+    });
+    it('can have child selectors with comma with @media query', function () {
+        styleToClassName({}, shadowConfig, {
+            '@media screen and (-ms-high-contrast: active), (-ms-high-contrast: none)': {
+                background: 'red',
+            },
+        });
+        expect(_stylesheet.getRules()).toEqual('@media screen and (-ms-high-contrast: active), (-ms-high-contrast: none){.css-0{background:red;}}');
+    });
+    it('can have same element class selectors', function () {
+        styleToClassName({}, shadowConfig, {
+            selectors: {
+                '&.foo': [{ background: 'red' }],
+            },
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0.foo{background:red;}');
+    });
+    it('can have same element class selectors without the selectors wrapper', function () {
+        styleToClassName({}, shadowConfig, {
+            '&.foo': [{ background: 'red' }],
+        });
+        expect(_stylesheet.getRules()).toEqual('.css-0.foo{background:red;}');
+    });
+    it('can register pseudo selectors', function () {
+        var className = styleToClassName({}, shadowConfig, {
+            selectors: {
+                ':hover': { background: 'red' },
+            },
+        });
+        expect(className).toEqual('css-0');
+        expect(_stylesheet.getRules()).toEqual('.css-0:hover{background:red;}');
+    });
+    it('can register pseudo selectors without the selectors wrapper', function () {
+        var className = styleToClassName({}, shadowConfig, {
+            ':hover': { background: 'red' },
+        });
+        expect(className).toEqual('css-0');
+        expect(_stylesheet.getRules()).toEqual('.css-0:hover{background:red;}');
+    });
+    it('can register parent and sibling selectors', function () {
+        var className = styleToClassName({}, shadowConfig, {
+            selectors: {
+                '& .child': { background: 'red' },
+                '.parent &': { background: 'green' },
