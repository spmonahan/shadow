+import { mergeCss } from './mergeStyles';
+import { Stylesheet, InjectionMode } from './Stylesheet';
+var _stylesheet = Stylesheet.getInstance();
+_stylesheet.setConfig({ injectionMode: InjectionMode.none });
+describe('mergeStyles', function () {
+    beforeEach(function () {
+        _stylesheet.reset();
+    });
+    it('can register left when passed a standalone object', function () {
+        mergeCss({ left: 10 });
+        expect(_stylesheet.getRules()).toEqual('.css-0{left:10px;}');
+    });
+    it('can register left when passed an array of objects', function () {
+        mergeCss([{ left: 10 }]);
+        expect(_stylesheet.getRules()).toEqual('.css-0{left:10px;}');
+    });
+    it('can register left in rtl', function () {
+        mergeCss({ left: 10 }, { rtl: true });
+        expect(_stylesheet.getRules()).toEqual('.css-0{right:10px;}');
+    });
+    it('can re-register rules when rtl is flipped', function () {
+        var result1 = mergeCss({ left: 10 }, {});
+        expect(_stylesheet.getRules()).toEqual('.css-0{left:10px;}');
+        expect(result1).toEqual('css-0');
+        var result2 = mergeCss({ left: 10 }, { rtl: true });
+        expect(_stylesheet.getRules()).toEqual('.css-0{left:10px;}.css-1{right:10px;}');
+        expect(result2).toEqual('css-1');
