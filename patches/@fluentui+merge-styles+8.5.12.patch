+        var className = mergeCss({ left: 1 });
+        var _a = extractStyleParts(shadowConfig, className, { left: 2 }), classes = _a.classes, objects = _a.objects;
+        expect(classes).toEqual([]);
+        expect(objects).toEqual([{ left: 1 }, { left: 2 }]);
