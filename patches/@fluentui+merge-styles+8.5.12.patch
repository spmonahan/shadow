+var _stylesheet = Stylesheet.getInstance();
+_stylesheet.setConfig({ injectionMode: InjectionMode.none });
+var shadowConfig;
+describe('extractStyleParts', function () {
+    beforeEach(function () {
+        _stylesheet.reset();
+        shadowConfig = { stylesheetKey: '__globalTest__', inShadow: false };
+    });
+    it('can extract classes and objects', function () {
+        var _a = extractStyleParts(shadowConfig, 'a', 'b', ['c', 'd'], { left: 1 }, [
+            'e',
+            { left: 2 },
+            { left: 3 },
+        ]), classes = _a.classes, objects = _a.objects;
