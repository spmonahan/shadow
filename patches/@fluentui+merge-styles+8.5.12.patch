+"use strict";
+Object.defineProperty(exports, "__esModule", { value: true });
+var tokenizeWithParentheses_1 = require("./tokenizeWithParentheses");
+describe('tokenizeWithParentheses', function () {
+    it('handles both space and tab', function () {
+        expect((0, tokenizeWithParentheses_1.tokenizeWithParentheses)('one two\tthree four')).toEqual(['one', 'two', 'three', 'four']);
+    });
+    it('collapses consecutive whitespace characters', function () {
+        expect((0, tokenizeWithParentheses_1.tokenizeWithParentheses)('  hello   world   ')).toEqual(['hello', 'world']);
+    });
+    it('returns an empty array for empty string', function () {
+        expect((0, tokenizeWithParentheses_1.tokenizeWithParentheses)('')).toEqual([]);
+    });
+    it('returns an empty array for a string containing only whitespace', function () {
+        expect((0, tokenizeWithParentheses_1.tokenizeWithParentheses)('\t \t  ')).toEqual([]);
+    });
+    it('treats parenthesized expressions as a single token', function () {
+        expect((0, tokenizeWithParentheses_1.tokenizeWithParentheses)('(a ) calc(b  / (var(--c)+d)) e')).toEqual(['(a )', 'calc(b  / (var(--c)+d))', 'e']);
+    });
+    it('handles mismatched parentheses', function () {
+        expect((0, tokenizeWithParentheses_1.tokenizeWithParentheses)('a) b))  (c d e)  (f  g')).toEqual(['a)', 'b))', '(c d e)', '(f  g']);
+    });
