+import * as React from 'react';
+import { renderHook } from '@testing-library/react-hooks';
+import { useEventCallback } from './useEventCallback';
+describe('useEventCallback', function () {
+    it('should not invalidate after rerender on unstable props', function () {
+        var unstableProp = [1, 1];
+        var callback = function () { return unstableProp.reduce(function (acc, item) { return acc + item; }, 0); };
+        var _a = renderHook(function () { return useEventCallback(callback); }), result = _a.result, rerender = _a.rerender;
+        var firstRender = result.current;
+        var firstRenderResult = firstRender();
+        unstableProp = [1, 1, 1];
+        rerender();
+        var secondRender = result.current;
+        var secondRenderResult = secondRender();
+        expect(firstRender).toBe(secondRender);
+        expect(firstRenderResult).toBe(2);
+        expect(secondRenderResult).toBe(3);
+    });
+    it('should not invalidate after rerender on unstable callback', function () {
+        var prop = [1, 1];
+        var unstableCallback = function () { return prop.reduce(function (acc, item) { return acc + item; }, 0); };
+        var _a = renderHook(function () { return useEventCallback(unstableCallback); }), result = _a.result, rerender = _a.rerender;
+        var firstRender = result.current;
+        var firstRenderResult = firstRender();
+        unstableCallback = function () { return prop.reduce(function (acc, item) { return (acc + item) * 0; }, 0); };
+        rerender();
+        var secondRender = result.current;
+        var secondRenderResult = secondRender();
+        expect(firstRender).toBe(secondRender);
+        expect(firstRenderResult).toBe(2);
+        expect(secondRenderResult).toBe(0);
+    });
+    it('should run before other layout effects', function () {
+        var useTestHook = function () {
+            var callback = useEventCallback(jest.fn());
+            // eslint-disable-next-line no-restricted-properties
+            React.useLayoutEffect(function () { return callback(); }, [callback]);
+        };
+        var result = renderHook(function () { return useTestHook(); }).result;
+        expect(result.error).toBeUndefined();
+    });
+});
