+import { __assign } from "tslib";
+import * as React from 'react';
+import { mount } from 'enzyme';
+import { setWarningCallback } from '@fluentui/utilities';
+import { useWarnings } from './useWarnings';
+// These tests don't cover the core warning utilities (which have their own tests), just the
+// integration with the hook and usage within function components.
+describe('useWarnings', function () {
+    var warn = jest.fn();
+    var getLastWarning = function () { return warn.mock.calls.slice(-1)[0][0]; };
+    var warningOptions;
+    var renderCount = 0;
+    var TestComponent = function (props) {
+        useWarnings(__assign(__assign({}, warningOptions), { name: 'TestComponent', props: props }));
+        renderCount++;
+        return React.createElement("div", null);
+    };
+    function validateWarnOnFirstRender(props, warningMessage) {
+        var wrapper = mount(React.createElement(TestComponent, __assign({}, props)));
+        expect(warn).toHaveBeenCalledTimes(1);
+        expect(warn).toHaveBeenLastCalledWith(warningMessage);
+        // ensure all the props change in case there was a dep on any of them where there shouldn't be
+        wrapper.setProps({
+            value: 'new',
+            defaultValue: 'updated',
+            deprecated: (props.deprecated || 0) + 1,
+            onChange: function () { return undefined; },
+            readOnly: !props.readOnly,
+        });
+        expect(renderCount).toBe(2);
+        expect(warn).toHaveBeenCalledTimes(1);
+    }
+    beforeEach(function () {
+        setWarningCallback(warn);
+    });
+    afterEach(function () {
+        warn.mockReset();
+        renderCount = 0;
+        setWarningCallback(undefined);
+        warningOptions = undefined;
+    });
+    it('only does generic warnings on first render', function () {
+        warningOptions = { other: ['oh no'] };
+        validateWarnOnFirstRender({}, 'oh no');
+    });
+    it('only warns for conditionally required props on first render', function () {
+        warningOptions = {
+            conditionallyRequired: [{ requiredProps: ['onChange'], conditionalPropName: 'value', condition: true }],
+        };
+        validateWarnOnFirstRender({ value: 'hi' }, "TestComponent property 'onChange' is required when 'value' is used.'");
+    });
+    it('only warns for deprecations on first render', function () {
+        warningOptions = { deprecations: { deprecated: 'n/a' } };
+        validateWarnOnFirstRender({ deprecated: 1 }, "TestComponent property 'deprecated' was used but has been deprecated. Use 'n/a' instead.");
+    });
+    it('only warns for mutually exclusive on first render', function () {
+        warningOptions = { mutuallyExclusive: { value: 'defaultValue' } };
+        validateWarnOnFirstRender({ value: 'foo', defaultValue: 'bar' }, "TestComponent property 'value' is mutually exclusive with 'defaultValue'. Use one or the other.");
+    });
+    it('warns for controlled usage whenever props update', function () {
