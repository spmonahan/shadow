+import * as React from 'react';
+import { act } from 'react-dom/test-utils';
+import { mount } from 'enzyme';
+import { useBoolean } from './useBoolean';
+import { validateHookValueNotChanged } from './testUtilities';
+describe('useBoolean', function () {
+    it('respects initial value', function () {
+        var value;
+        var TestComponent = function (_a) {
+            var initialValue = _a.initialValue;
+            value = useBoolean(initialValue)[0];
+            return React.createElement("div", null);
+        };
+        mount(React.createElement(TestComponent, { initialValue: true }));
+        expect(value).toBe(true);
+        mount(React.createElement(TestComponent, { initialValue: false }));
+        expect(value).toBe(false);
+    });
+    validateHookValueNotChanged('returns the same callbacks', function () {
+        var _a = useBoolean(true), _b = _a[1], setTrue = _b.setTrue, setFalse = _b.setFalse, toggle = _b.toggle;
+        return [setTrue, setFalse, toggle];
+    });
+    it('updates the value', function () {
+        var value;
+        var callbacks;
+        var TestComponent = function () {
+            var _a;
+            _a = useBoolean(true), value = _a[0], callbacks = _a[1];
+            return React.createElement("div", null, value);
+        };
+        mount(React.createElement(TestComponent, null));
+        expect(value).toBe(true);
+        act(function () { return callbacks.setFalse(); });
+        expect(value).toBe(false);
+        act(function () { return callbacks.setFalse(); });
+        expect(value).toBe(false);
+        act(function () { return callbacks.setTrue(); });
+        expect(value).toBe(true);
+    });
+    it('handles toggling', function () {
+        var value;
+        var callbacks;
+        var TestComponent = function () {
+            var _a;
+            _a = useBoolean(true), value = _a[0], callbacks = _a[1];
+            return React.createElement("div", null);
+        };
+        mount(React.createElement(TestComponent, null));
+        // Toggle the value
+        act(function () { return callbacks.toggle(); });
+        // correct new value
+        expect(value).toBe(false);
+        // Toggle again
+        act(function () { return callbacks.toggle(); });
+        expect(value).toBe(true);
+    });
+});
