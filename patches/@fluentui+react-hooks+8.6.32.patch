+import * as React from 'react';
+import { mount } from 'enzyme';
+import { useMergedRefs } from './useMergedRefs';
+describe('useMergedRefs', function () {
+    var wrapper;
+    afterEach(function () {
+        if (wrapper && wrapper.exists()) {
+            wrapper.unmount();
+            wrapper = undefined;
+        }
+    });
+    it('always returns the same ref (refs should be immutable)', function () {
+        var lastMergedRef;
+        var refFunc = function () { return null; };
+        var TestComponent = function () {
+            lastMergedRef = useMergedRefs(refFunc);
+            return null;
+        };
+        wrapper = mount(React.createElement(TestComponent, null));
+        var ref1 = lastMergedRef;
+        wrapper.setProps({});
+        var ref2 = lastMergedRef;
+        expect(ref1).toBe(ref2);
+    });
+    it('always mutates the ref when 1 or more merged refs mutate', function () {
+        var lastMergedRef;
+        var TestComponent = function () {
+            lastMergedRef = useMergedRefs(function () { return ({}); });
+            return null;
+        };
+        wrapper = mount(React.createElement(TestComponent, null));
+        var ref1 = lastMergedRef;
+        wrapper.setProps({});
+        var ref2 = lastMergedRef;
+        expect(ref1).not.toBe(ref2);
+    });
+    it('updates all provided refs', function () {
+        var refObject = React.createRef();
+        var refValue = null;
+        var TestComponent = function () {
+            var mergedRef = useMergedRefs(refObject, function (val) { return (refValue = val); });
+            mergedRef(true);
+            return null;
+        };
+        wrapper = mount(React.createElement(TestComponent, null));
+        expect(refObject.current).toBe(true);
+        expect(refValue).toBe(true);
+    });
+    it('updates the current property', function () {
+        var mergedRef = undefined;
+        var TestComponent = function () {
+            mergedRef = useMergedRefs(React.useRef(''), React.useRef(''));
+            mergedRef('123');
+            return null;
+        };
+        wrapper = mount(React.createElement(TestComponent, null));
+        expect(mergedRef).toBeTruthy();
+        expect(mergedRef.current).toEqual('123');
+    });
+    it('reuses the same ref callback if refs remain stable', function () {
+        var refObject = React.createRef();
+        // eslint-disable-next-line @typescript-eslint/no-empty-function
+        var refValueFunc = function (val) { };
+        var refCallback = undefined;
+        var TestComponent = function () {
+            refCallback = useMergedRefs(refObject, refValueFunc);
+            return null;
+        };
+        wrapper = mount(React.createElement(TestComponent, null));
+        var firstRefCallback = refCallback;
+        // Re-render the component
+        wrapper.update();
+        expect(refCallback).toBe(firstRefCallback);
+    });
+    it('handles changing ref callbacks', function () {
+        var refObject = React.createRef();
+        var firstRefValue = null;
+        var refValueFunc = function (val) { return (firstRefValue = val); };
+        var TestComponent = function () {
+            var mergedRef = useMergedRefs(refObject, refValueFunc);
+            mergedRef(true);
+            return null;
+        };
+        wrapper = mount(React.createElement(TestComponent, null));
+        var secondRefValue = null;
+        refValueFunc = function (val) { return (secondRefValue = val); };
+        // Re-render the component
+        wrapper.setProps({});
+        expect(firstRefValue).toBe(true);
+        expect(secondRefValue).toBe(true);
+    });
+});
