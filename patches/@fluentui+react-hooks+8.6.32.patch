+import * as React from 'react';
+import { useSetTimeout } from './useSetTimeout';
+import { safeMount } from '@fluentui/test-utilities';
+import { validateHookValueNotChanged } from './testUtilities';
+describe('useSetTimeout', function () {
+    // Initialization
+    var timesCalled = 0;
+    beforeAll(function () {
+        jest.useFakeTimers();
+    });
+    afterAll(function () {
+        jest.useRealTimers();
+    });
+    afterEach(function () {
+        timesCalled = 0;
+    });
+    var TestComponent = React.forwardRef(function (props, ref) {
+        var _a = useSetTimeout(), setTimeout = _a.setTimeout, clearTimeout = _a.clearTimeout;
+        var state = React.useRef({ id: 0 }).current;
+        React.useImperativeHandle(ref, function () { return ({
+            clearTimeout: function () { return clearTimeout(state.id); },
+        }); }, [clearTimeout, state]);
+        state.id = setTimeout(function () {
+            timesCalled++;
+        }, 0);
+        return React.createElement("div", null);
+    });
+    it('updates value when mounted', function () {
+        safeMount(React.createElement(TestComponent, null), function () {
+            expect(timesCalled).toEqual(0);
+            jest.runOnlyPendingTimers();
+            expect(timesCalled).toEqual(1);
+            jest.runOnlyPendingTimers();
+            expect(timesCalled).toEqual(1);
+        });
+    });
+    validateHookValueNotChanged('returns the same callbacks each time', function () {
+        var _a = useSetTimeout(), setTimeout = _a.setTimeout, clearTimeout = _a.clearTimeout;
+        return [setTimeout, clearTimeout];
+    });
+    it('does not execute the timeout when unmounted', function () {
+        safeMount(React.createElement(TestComponent, null), function (wrapper) {
+            expect(timesCalled).toEqual(0);
+            wrapper.unmount();
+            jest.runOnlyPendingTimers();
+            expect(timesCalled).toEqual(0);
+        });
+    });
+    it('can cancel timeout', function () {
+        var ref = React.createRef();
+        safeMount(React.createElement(TestComponent, { ref: ref }), function () {
+            jest.runOnlyPendingTimers();
+            expect(timesCalled).toEqual(1);
+            ref.current.clearTimeout();
+            jest.runOnlyPendingTimers();
+            expect(timesCalled).toEqual(1);
+        });
+    });
+});
