+import * as React from 'react';
+import { mount } from 'enzyme';
+import { useConst } from './useConst';
+import { validateHookValueNotChanged } from './testUtilities';
+describe('useConst', function () {
+    validateHookValueNotChanged('returns the same value with value initializer', function () { return [useConst(Math.random())]; });
+    validateHookValueNotChanged('returns the same value with function initializer', function () { return [
+        useConst(function () { return Math.random(); }),
+    ]; });
+    it('calls the function initializer only once', function () {
+        var initializer = jest.fn(function () { return Math.random(); });
+        var TestComponent = function () {
+            var value = useConst(initializer);
+            return React.createElement("div", null, value);
+        };
+        var wrapper = mount(React.createElement(TestComponent, null));
+        var firstValue = wrapper.text();
+        // Re-render the component
+        wrapper.update();
+        // Text should be the same
+        expect(wrapper.text()).toBe(firstValue);
+        // Function shouldn't have been called again
+        expect(initializer).toHaveBeenCalledTimes(1);
+    });
+    it('works with a function initializer which returns undefined', function () {
+        var initializer = jest.fn(function () { return undefined; });
+        var TestComponent = function () {
+            var value = useConst(initializer);
+            return React.createElement("div", null, value);
+        };
+        var wrapper = mount(React.createElement(TestComponent, null));
+        // Re-render the component
+        wrapper.update();
+        // Function shouldn't have been called again
+        expect(initializer).toHaveBeenCalledTimes(1);
+    });
+});
+//# sourceMappingURL=useConst.test.js.map
